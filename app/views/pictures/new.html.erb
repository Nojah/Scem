<h1>New picture</h1>

<% form_for @picture, :html => { :multipart => true } do |f| %>

  <%=  hidden_field_tag :parent_id, params[:parent_id] %>
  <%=  hidden_field_tag :parent_type, params[:parent_type] %>

<%= render :partial => 'form', :locals => {:f => f} %>

  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', pictures_path %>